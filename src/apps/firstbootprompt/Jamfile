	BootPrompt.cpp
	BootPromptWindow.cpp
	Keymap.cpp
	KeymapListItem.cpp
	: be translation shared [ TargetLibstdc++ ] localestub
	: BootPrompt.rdef
;

DoCatalogs FirstBootPrompt :
	x-vnd.Haiku-FirstBootPrompt
	:
	BootPrompt.cpp
	BootPromptWindow.cpp
;

