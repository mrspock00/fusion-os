SubDir HAIKU_TOP src add-ons kernel bus_managers usb ;

SubDirC++Flags -fno-rtti ;

UsePrivateKernelHeaders ;

KernelStaticLibrary libusb.a :
	Stack.cpp
	Device.cpp
	Hub.cpp
	BusManager.cpp
	Transfer.cpp
	Pipe.cpp
	Interface.cpp
	Object.cpp
	PhysicalMemoryAllocator.cpp
;

KernelAddon usb :
	usb.cpp
	: libusb.a
	: usb.rdef
	;
