
resource(0, "BEOS:TYPE") #'MIMS' "application/x-vnd.Be-meta-mime";

resource(1, "META:TYPE") "audio/mpeg";

resource(4, "META:S:DESC") #'MSDC' "MPEG audio file";

resource(5, "META:EXTENS") message(234) {
	"extensions" = "mp2",
	"extensions" = "MP2",
	"extensions" = "mp3",
	"extensions" = "MP3",
	"type" = "audio/mpeg"
};

resource(6, "META:SNIFF_RULE") "0.50 (0xfffb|0xfffa|\"ID3\")";
